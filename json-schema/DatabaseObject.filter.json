{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"type": "object",
	"$dynamicAnchor": "obj",
	"patternProperties": {
		".": {
			"anyOf": [
				{ "type": "string" },
				{ "type": "number" },
				{ "type": "null" },
				{
					"type": "array",
					"minItems": 1,
					"items": {
						"type": "object",
						"$dynamicRef": "#obj"
					}
				},
				{
					"type": "array",
					"minItems": 2,
					"prefixItems": [
						{
							"type": "string",
							"pattern": "^(in|between|<|>|<>|=)(:(escaped|unescaped))?$"
						}
					],
					"items": {
						"anyOf": [
							{ "type": "string" },
							{ "type": "number" },
							{ "type": "null" }
						]
					}
				}
			]
		}
	}
}
